{
  "version": "1.0.0",
  "project": "a11yscope",
  "gateway": {
    "port": 18789,
    "host": "127.0.0.1"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-6",
        "fallbacks": ["anthropic/claude-haiku-4-5"]
      },
      "maxTokens": 8192,
      "temperature": 0.3
    }
  },
  "channels": {
    "terminal": {
      "enabled": true
    },
    "web": {
      "enabled": true,
      "port": 18789
    }
  },
  "sandbox": {
    "allowedPaths": [
      "/home/user/accessguard"
    ],
    "blockedCommands": [
      "rm -rf /",
      "git push --force"
    ],
    "networkAccess": true
  },
  "skills": {
    "autoUpdate": false,
    "installed": []
  }
}
